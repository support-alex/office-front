AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;;;;AAKA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;;;AAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login-1.vue","sourceRoot":"src/view/pages/auth/login_pages","sourcesContent":["<template>\r\n  <div class=\"d-flex flex-column flex-root\">\r\n    <div\r\n      class=\"login login-1 d-flex flex-column flex-lg-row flex-column-fluid bg-white\"\r\n      :class=\"{\r\n        'login-signin-on': this.state == 'signin',\r\n        'login-signup-on': this.state == 'signup',\r\n        'login-forgot-on': this.state == 'forgot'\r\n      }\"\r\n      id=\"kt_login\"\r\n    >\r\n      <!--begin::Aside-->\r\n      <div\r\n        class=\"login-aside d-flex flex-column flex-row-auto\"\r\n        style=\"background-color: #F2C98A;\"\r\n      >\r\n        <div class=\"d-flex flex-column-auto flex-column pt-lg-40 pt-15\">\r\n          <a href=\"#\" class=\"text-center mb-10\">\r\n            <img\r\n              src=\"media/logos/logo-letter-1.png\"\r\n              class=\"max-h-70px\"\r\n              alt=\"\"\r\n            />\r\n          </a>\r\n          <h3\r\n            class=\"font-weight-bolder text-center font-size-h4 font-size-h1-lg\"\r\n            style=\"color: #986923;\"\r\n          >\r\n            Discover Amazing Metronic <br />with great build tools\r\n          </h3>\r\n        </div>\r\n        <div\r\n          class=\"aside-img d-flex flex-row-fluid bgi-no-repeat bgi-position-y-bottom bgi-position-x-center\"\r\n          :style=\"{ backgroundImage: `url(${backgroundImage})` }\"\r\n        ></div>\r\n      </div>\r\n      <!--begin::Aside-->\r\n      <!--begin::Content-->\r\n      <div\r\n        class=\"login-content flex-row-fluid d-flex flex-column justify-content-center position-relative overflow-hidden p-7 mx-auto\"\r\n      >\r\n        <div class=\"d-flex flex-column-fluid flex-center\">\r\n          <!--begin::Signin-->\r\n          <div class=\"login-form login-signin\">\r\n            <form\r\n              class=\"form\"\r\n              novalidate=\"novalidate\"\r\n              id=\"kt_login_signin_form\"\r\n            >\r\n              <div class=\"pb-13 pt-lg-0 pt-5\">\r\n                <h3\r\n                  class=\"font-weight-bolder text-dark font-size-h4 font-size-h1-lg\"\r\n                >\r\n                  Welcome to Metronic\r\n                   <div @click=\"test()\"  class=\"btn btn-success\"  >\r\n                \r\n                  Log in\r\n                </div>\r\n                </h3>\r\n                <span class=\"text-muted font-weight-bold font-size-h4\"\r\n                  >New Here?\r\n                  <a\r\n                    id=\"kt_login_signup\"\r\n                    class=\"text-primary font-weight-bolder\"\r\n                    @click=\"showForm('signup')\"\r\n                    >Create an Account</a\r\n                  ></span\r\n                >\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label class=\"font-size-h6 font-weight-bolder text-dark\"\r\n                  >Email</label\r\n                >\r\n                <div\r\n                  id=\"example-input-group-1\"\r\n                  label=\"\"\r\n                  label-for=\"example-input-1\"\r\n                >\r\n                  <input\r\n                    class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg\"\r\n                    type=\"text\"\r\n                    name=\"email\"\r\n                    ref=\"email\"\r\n                    v-model=\"form.email\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <div class=\"d-flex justify-content-between mt-n5\">\r\n                  <label class=\"font-size-h6 font-weight-bolder text-dark pt-5\"\r\n                    >Password</label\r\n                  >\r\n                  <a\r\n                    class=\"text-primary font-size-h6 font-weight-bolder text-hover-primary pt-5\"\r\n                    id=\"kt_login_forgot\"\r\n                    @click=\"showForm('forgot')\"\r\n                    >Forgot Password ?</a\r\n                  >\r\n                </div>\r\n                <div\r\n                  id=\"example-input-group-2\"\r\n                  label=\"\"\r\n                  label-for=\"example-input-2\"\r\n                >\r\n                  <input\r\n                    class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg\"\r\n                    type=\"password\"\r\n                    name=\"password\"\r\n                    ref=\"password\"\r\n                    v-model=\"form.password\"\r\n                    autocomplete=\"off\"\r\n                  />\r\n                </div>\r\n              </div>\r\n              <div class=\"pb-lg-0 pb-5\">\r\n                <button\r\n                  ref=\"kt_login_signin_submit\"\r\n                  class=\"btn btn-primary font-weight-bolder font-size-h6 px-15 py-4 my-3 mr-3\"\r\n                >\r\n                  Sign In\r\n                </button>\r\n             \r\n\r\n                <!-- <button\r\n                  type=\"button\"\r\n                  class=\"btn btn-light-primary font-weight-bolder px-8 py-4 my-3 font-size-lg\"\r\n                >\r\n                  <span class=\"svg-icon svg-icon-md\">\r\n                    <inline-svg\r\n                      src=\"media/svg/social-icons/google.svg\"\r\n                    /> </span\r\n                  >Sign in with Google\r\n                </button> -->\r\n                <GoogleLogin :params=\"params\" :renderParams=\"renderParams\" :onSuccess=\"onSuccess\" :onFailure=\"onFailure\"></GoogleLogin>\r\n              </div>\r\n            </form>\r\n          </div>\r\n          <!--end::Signin-->\r\n          <!--begin::Signup-->\r\n          <div class=\"login-form login-signup\">\r\n            <form\r\n              class=\"form\"\r\n              novalidate=\"novalidate\"\r\n              id=\"kt_login_signup_form\"\r\n            >\r\n              <div class=\"pb-13 pt-lg-0 pt-5\">\r\n                <h3\r\n                  class=\"font-weight-bolder text-dark font-size-h4 font-size-h1-lg\"\r\n                >\r\n                  Sign Up\r\n                </h3>\r\n                <p class=\"text-muted font-weight-bold font-size-h4\">\r\n                  Enter your details to create your account\r\n                </p>\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg font-size-h6\"\r\n                  type=\"text\"\r\n                  placeholder=\"Fullname\"\r\n                  name=\"fullname\"\r\n                  ref=\"fullname\"\r\n                  autocomplete=\"off\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg font-size-h6\"\r\n                  type=\"email\"\r\n                  placeholder=\"Email\"\r\n                  name=\"email\"\r\n                  ref=\"remail\"\r\n                  autocomplete=\"off\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg font-size-h6\"\r\n                  type=\"password\"\r\n                  placeholder=\"Password\"\r\n                  name=\"password\"\r\n                  ref=\"rpassword\"\r\n                  autocomplete=\"off\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg font-size-h6\"\r\n                  type=\"password\"\r\n                  placeholder=\"Confirm password\"\r\n                  name=\"cpassword\"\r\n                  ref=\"cpassword\"\r\n                  autocomplete=\"off\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label class=\"checkbox mb-0\">\r\n                  <input type=\"checkbox\" name=\"agree\" />\r\n                  <span class=\"mr-2\"></span>\r\n                  I Agree the\r\n                  <a href=\"#\" class=\"ml-2\">terms and conditions</a>.\r\n                </label>\r\n              </div>\r\n              <div class=\"form-group d-flex flex-wrap pb-lg-0 pb-3\">\r\n                <button\r\n                  ref=\"kt_login_signup_submit\"\r\n                  class=\"btn btn-primary font-weight-bolder font-size-h6 px-8 py-4 my-3 mr-4\"\r\n                  style=\"width:150px;\"\r\n                >\r\n                  Submit\r\n                </button>\r\n                <button\r\n                  type=\"button\"\r\n                  id=\"kt_login_signup_cancel\"\r\n                  class=\"btn btn-light-primary font-weight-bolder font-size-h6 px-8 py-4 my-3\"\r\n                  @click=\"showForm('signin')\"\r\n                >\r\n                  Cancel\r\n                </button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n          <!--end::Signup-->\r\n          <!--begin::Forgot-->\r\n          <div class=\"login-form login-forgot\">\r\n            <!--begin::Form-->\r\n            <form\r\n              class=\"form\"\r\n              novalidate=\"novalidate\"\r\n              id=\"kt_login_forgot_form\"\r\n              ref=\"kt_login_forgot_form\"\r\n            >\r\n              <div class=\"pb-13 pt-lg-0 pt-5\">\r\n                <h3\r\n                  class=\"font-weight-bolder text-dark font-size-h4 font-size-h1-lg\"\r\n                >\r\n                  Forgotten Password ?\r\n                </h3>\r\n                <p class=\"text-muted font-weight-bold font-size-h4\">\r\n                  Enter your email to reset your password\r\n                </p>\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <input\r\n                  class=\"form-control form-control-solid h-auto py-7 px-6 rounded-lg font-size-h6\"\r\n                  type=\"email\"\r\n                  placeholder=\"Email\"\r\n                  name=\"email\"\r\n                  autocomplete=\"off\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group d-flex flex-wrap pb-lg-0\">\r\n                <button\r\n                  type=\"button\"\r\n                  id=\"kt_login_forgot_submit\"\r\n                  class=\"btn btn-primary font-weight-bolder font-size-h6 px-8 py-4 my-3 mr-4\"\r\n                >\r\n                  Submit\r\n                </button>\r\n                <button\r\n                  type=\"button\"\r\n                  id=\"kt_login_forgot_cancel\"\r\n                  class=\"btn btn-light-primary font-weight-bolder font-size-h6 px-8 py-4 my-3\"\r\n                  @click=\"showForm('signin')\"\r\n                >\r\n                  Cancel\r\n                </button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n          <!--end::Forgot-->\r\n        </div>\r\n        <!--begin::Content footer-->\r\n        <div\r\n          class=\"d-flex justify-content-lg-start justify-content-center align-items-end py-7 py-lg-0\"\r\n        >\r\n          <a href=\"#\" class=\"text-primary font-weight-bolder font-size-h5\"\r\n            >Terms</a\r\n          >\r\n          <a href=\"#\" class=\"text-primary ml-10 font-weight-bolder font-size-h5\"\r\n            >Plans</a\r\n          >\r\n          <a href=\"#\" class=\"text-primary ml-10 font-weight-bolder font-size-h5\"\r\n            >Contact Us</a\r\n          >\r\n        </div>\r\n        <!--end::Content footer-->\r\n      </div>\r\n      <!--end::Content-->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<!-- Load login custom page styles -->\r\n<style lang=\"scss\">\r\n@import \"@/assets/sass/pages/login/login-1.scss\";\r\n</style>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\n\r\n\r\nimport formValidation from \"@/assets/plugins/formvalidation/dist/es6/core/Core\";\r\n\r\n// FormValidation plugins\r\nimport Trigger from \"@/assets/plugins/formvalidation/dist/es6/plugins/Trigger\";\r\nimport Bootstrap from \"@/assets/plugins/formvalidation/dist/es6/plugins/Bootstrap\";\r\nimport SubmitButton from \"@/assets/plugins/formvalidation/dist/es6/plugins/SubmitButton\";\r\n\r\nimport KTUtil from \"@/assets/js/components/util\";\r\nimport { mapGetters, mapState } from \"vuex\";\r\nimport { LOGIN, LOGOUT, REGISTER } from \"@/core/services/store/auth.module\";\r\nimport Swal from \"sweetalert2\";\r\nimport GoogleLogin from 'vue-google-login';\r\nexport default {\r\n  name: \"login-1\",\r\n  data() {\r\n    return {\r\n       // client_id is the only required property but you can add several more params, full list down bellow on the Auth api section\r\n        params: {\r\n            client_id: \"753065350883-ocbit7gouqcpe5ifpvv615f3cv03r27d.apps.googleusercontent.com\"\r\n        },\r\n        // only needed if you want to render the button with the google ui\r\n        renderParams: {\r\n            width: 250,\r\n            height: 50,\r\n            longtitle: true\r\n        },\r\n   \r\n      state: \"signin\",\r\n      // Remove this dummy login info\r\n      form: {\r\n        email: \"admin@demo.com\",\r\n        password: \"demo\"\r\n      }\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      errors: state => state.auth.errors\r\n    }),\r\n    ...mapGetters([\"currentUser\"]),\r\n\r\n    backgroundImage() {\r\n      return (\r\n        process.env.BASE_URL + \"media/svg/illustrations/login-visual-1.svg\"\r\n      );\r\n    }\r\n  },\r\n  mounted() {\r\n    const signin_form = KTUtil.getById(\"kt_login_signin_form\");\r\n    const signup_form = KTUtil.getById(\"kt_login_signup_form\");\r\n    const forgot_form = KTUtil.getById(\"kt_login_forgot_form\");\r\n\r\n    this.fv = formValidation(signin_form, {\r\n      fields: {\r\n        email: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Username is required\"\r\n            }\r\n          }\r\n        },\r\n        password: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Password is required\"\r\n            }\r\n          }\r\n        }\r\n      },\r\n      plugins: {\r\n        trigger: new Trigger(),\r\n        submitButton: new SubmitButton(),\r\n        bootstrap: new Bootstrap()\r\n      }\r\n    });\r\n\r\n    this.fv1 = formValidation(signup_form, {\r\n      fields: {\r\n        fullname: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Full name is required\"\r\n            }\r\n          }\r\n        },\r\n        email: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Email is required\"\r\n            },\r\n            emailAddress: {\r\n              message: \"The value is not a valid email address\"\r\n            }\r\n          }\r\n        },\r\n        password: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Password is required\"\r\n            }\r\n          }\r\n        },\r\n        cpassword: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Confirm password is required\"\r\n            },\r\n            identical: {\r\n              compare: function() {\r\n                return signup_form.querySelector('[name=\"password\"]').value;\r\n              },\r\n              message: \"The password and its confirm are not the same\"\r\n            }\r\n          }\r\n        },\r\n        agree: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"You should agree terms and conditions\"\r\n            }\r\n          }\r\n        }\r\n      },\r\n      plugins: {\r\n        trigger: new Trigger(),\r\n        submitButton: new SubmitButton(),\r\n        bootstrap: new Bootstrap()\r\n      }\r\n    });\r\n\r\n    this.fv2 = formValidation(forgot_form, {\r\n      fields: {\r\n        email: {\r\n          validators: {\r\n            notEmpty: {\r\n              message: \"Email is required\"\r\n            },\r\n            emailAddress: {\r\n              message: \"The value is not a valid email address\"\r\n            }\r\n          }\r\n        }\r\n      },\r\n      plugins: {\r\n        trigger: new Trigger(),\r\n        submitButton: new SubmitButton(),\r\n        bootstrap: new Bootstrap()\r\n      }\r\n    });\r\n\r\n    this.fv.on(\"core.form.valid\", () => {\r\n      var email = this.form.email;\r\n      var password = this.form.password;\r\n\r\n      // clear existing errors\r\n      this.$store.dispatch(LOGOUT);\r\n\r\n      // set spinner to submit button\r\n      const submitButton = this.$refs[\"kt_login_signin_submit\"];\r\n      submitButton.classList.add(\"spinner\", \"spinner-light\", \"spinner-right\");\r\n\r\n      // dummy delay\r\n      setTimeout(() => {\r\n        // send login request\r\n        this.$store\r\n          .dispatch(LOGIN, { email, password })\r\n          // go to which page after successfully login\r\n          .then(() => this.$router.push({ name: \"dashboard\" }))\r\n          .catch(() => {});\r\n\r\n        submitButton.classList.remove(\r\n          \"spinner\",\r\n          \"spinner-light\",\r\n          \"spinner-right\"\r\n        );\r\n      }, 2000);\r\n    });\r\n\r\n    this.fv.on(\"core.form.invalid\", () => {\r\n      Swal.fire({\r\n        title: \"\",\r\n        text: \"Please, provide correct data!\",\r\n        icon: \"error\",\r\n        confirmButtonClass: \"btn btn-secondary\",\r\n        heightAuto: false\r\n      });\r\n    });\r\n\r\n    this.fv1.on(\"core.form.valid\", () => {\r\n      const email = this.$refs.remail.value;\r\n      const password = this.$refs.rpassword.value;\r\n\r\n      // clear existing errors\r\n      this.$store.dispatch(LOGOUT);\r\n\r\n      // set spinner to submit button\r\n      const submitButton = this.$refs[\"kt_login_signup_submit\"];\r\n      submitButton.classList.add(\"spinner\", \"spinner-light\", \"spinner-right\");\r\n\r\n      // dummy delay\r\n      setTimeout(() => {\r\n        // send register request\r\n        this.$store\r\n          .dispatch(REGISTER, {\r\n            email: email,\r\n            password: password\r\n          })\r\n          .then(() => this.$router.push({ name: \"dashboard\" }));\r\n\r\n        submitButton.classList.remove(\r\n          \"spinner\",\r\n          \"spinner-light\",\r\n          \"spinner-right\"\r\n        );\r\n      }, 2000);\r\n    });\r\n\r\n    this.fv1.on(\"core.form.invalid\", () => {\r\n      Swal.fire({\r\n        title: \"\",\r\n        text: \"Please, provide correct data!\",\r\n        icon: \"error\",\r\n        confirmButtonClass: \"btn btn-secondary\",\r\n        heightAuto: false\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    async test() {\r\n      var id_token = { id_token: \"HELLO\"};\r\n      console.log(\"HELLO\", id_token);\r\n      // var res1 = null;\r\n\r\n      // this.$store.dispatch(GOOGLE_LOGIN, id_token).then((result) => console.log(\"result >>> \",result)  );\r\n      \r\n      var res1 = null;\r\n      try{\r\n\r\n              res1 =  await axios.get(\"https://api.openbrewerydb.org/breweries\");\r\n              console.log(\"res1\", res1);\r\n\r\n            }\r\n            catch (error) {\r\n              console.log(\"ERROR11111 >>>>> \", error);\r\n            }\r\n      \r\n\r\n      // try{\r\n      //   res1 =  await axios.post(\"http://localhost:8000/gauth/xxx\", id_token);\r\n      //   console.log(\"res1\", res1);\r\n      // }\r\n      // catch (error) {\r\n      //   console.log(\"ERROR11111 >>>>> \", error.response);\r\n      // }\r\n\r\n      // try {\r\n        // res1 =  axios.post(\"http://localhost:8000/gauth/xxx\", id_token)\r\n        //             .then((response) => {\r\n        //                console.log(\"response\", response);\r\n        //             }\r\n        //             )\r\n        //             .catch(error => {\r\n        //              console.log(\"error\", error);\r\n        //             }\r\n        //             )\r\n        //             ;\r\n        // console.log(\"response from server\", res1);\r\n      // } catch (error) {\r\n      //   console.log(\"ERROR11111 >>>>> \", error.response);\r\n      // }\r\n    },\r\n\r\n    async onSuccess(googleUser) {\r\n      // console.log(googleUser);\r\n \r\n      // This only gets the user in