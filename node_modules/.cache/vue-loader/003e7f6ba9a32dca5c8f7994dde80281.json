-height: 90vh; position: relative;\"\r\n        >\r\n          <KTMenu></KTMenu>\r\n        </perfect-scrollbar>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!-- end:: Aside -->\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport KTLayoutAside from \"@/assets/js/layout/base/aside.js\";\r\nimport KTLayoutAsideMenu from \"@/assets/js/layout/base/aside-menu.js\";\r\nimport KTMenu from \"@/view/layout/aside/Menu.vue\";\r\n\r\nexport default {\r\n  name: \"KTAside\",\r\n  data() {\r\n    return {\r\n      insideTm: 0,\r\n      outsideTm: 0\r\n    };\r\n  },\r\n  components: {\r\n    KTMenu\r\n  },\r\n  mounted() {\r\n    this.$nextTick(() => {\r\n      // Init Aside\r\n      KTLayoutAside.init(this.$refs[\"main_aside\"]);\r\n\r\n      // Init Aside Menu\r\n      KTLayoutAsideMenu.init(this.$refs[\"main_aside_menu\"]);\r\n    });\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"layoutConfig\", \"getClasses\"]),\r\n\r\n    /**\r\n     * Get extra classes for menu based on the options\r\n     */\r\n    asideMenuClass() {\r\n      const classes = this.getClasses(\"main_aside_menu\");\r\n      if (typeof classes !== \"undefined\") {\r\n        return classes.join(\" \");\r\n      }\r\n      return null;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}